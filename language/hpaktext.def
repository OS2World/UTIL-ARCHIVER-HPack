# The possible error codes

%begin errcodedef

%errcode EXIT_OK			0		# No error

%errcode EXIT_INT_ERR		1		# Internal error
%errcode EXIT_NO_MEM		2		# Out of memory
%errcode EXIT_NO_DISK		3		# Out of disk space
%errcode EXIT_NO_ARCH		4		# Can't open archive file
%errcode EXIT_NO_TEMP		5		# Can't open temporary file
%errcode EXIT_NO_PATH		6		# Can't find path
%errcode EXIT_NO_BASE		7		# Can't access base directory
%errcode EXIT_NO_MKDIR		8		# Can't create directory
%errcode EXIT_BREAK			9		# User interrupt
%errcode EXIT_FILE_ERR		10		# Unknown file error
%errcode EXIT_DIR_CORRUPT	11		# Archive directory corrupted

%errcode EXIT_LONG_PATH		100		# Path too long
%errcode EXIT_NO_OVERRIDE	101		# Can't override base path
%errcode EXIT_NEST			102		# Directories nested too deeply
%errcode EXIT_SCRIPT_ERR	103		# Error(s) in script file
%errcode EXIT_NOT_ARCH		104		# Not an HPACK archive
%errcode EXIT_BAD_KEYFILE	105		# Bad keyfile
%errcode EXIT_NOTHING		106		# Nothing to do
%errcode EXIT_COMMAND		107		# Unknown command
%errcode EXIT_OPTION		108		# Unknown option

%errcode EXIT_PASS			200		# Differing or incorrect password(s)
%errcode EXIT_CHANGE		201		# Bad attempt to change archive (eg attempt
									# to change a block-encrypted archive)
%errcode EXIT_NOLONG		202		# Long arg.format not supported
%errcode EXIT_BADWILD		203		# Bad wildcard format or inappropriate use
									# of wildcards
%errcode EXIT_SECURITY		204		# General security/encryption error
%errcode EXIT_NOCRYPT		205		# Cannot process encrypted archive

%end errcodedef

# All error messages are of the form: "Error: " + message.  For example
# the first error message, "Internal error", would be printed as:
# "Error: Internal error".

%begin severeerrors

# Severe errors

# Internal error: "Internal error"
%errcode EXIT_INT_ERR
en: Internal error
de: Interner Fehler
nl: Interne fout
it: Errore interno
pl: B$(sl)$(oa)d wewn$(oe)trzny
es: Error interno
ch: !!ch
bv: Interner F$(ua)lla

# Out of memory error: "Out of memory"
%errcode EXIT_NO_MEM
en: Out of memory
de: Kein Speicher mehr
fr: Memoire insuffisante
nl: Geheugen vol
it: Memoria insufficiente
pl: Brak pami$(oe)ci
es: Memoria insuficiente
ch: !!ch
bv: Koa Speicher mehr do

# Out of disk space error: "Out of disk space"
%errcode EXIT_NO_DISK
en: Out of disk space
de: Kein Plattenspeicher mehr
nl: Geen ruimte op de diskette
it: Spazio su disco insufficiente
pl: Brak miejsca na dysku
es: Espacio en disco insuficiente
ch: !!ch
bv: Koa Plattenspeicher mehr do

# Cannot open archive file <filename>: "Cannot open archive file %s"
%errcode EXIT_NO_ARCH
en: Cannot open archive file %s
de: Archivdatei %s kann nicht ge$(uo)ffnet werden
nl: Archief bestand %s can niet geopend worden
it: Impossibile aprire l' archivio %s
pl: Nie mog$(oe) otworzyc archiwum %s
es: No es posible abrir el archivo %s
ch: !!ch
bv: d'Archivdatei %s konn ned aufgmacht wern

# Cannot open temporary file: "Cannot open temp file"
